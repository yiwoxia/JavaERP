<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.situ.ssh.pojo" auto-import="true">
		<class name="Products" table="products">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="product_id" column="product_id" type="java.lang.String"></property>
			<property name="name" column="name" type="java.lang.String"></property>
			<property name="store_id" column="store_id" type="java.lang.Integer"></property>
			<property name="specification" column="specification" type="java.lang.String"></property>
			<property name="type" column="type" type="java.lang.String"></property>
			<property name="place" column="place" type="java.lang.String"></property>
			<property name="amount" column="amount" type="java.lang.Integer"></property>
			<property name="price" column="price" type="java.lang.Double"></property>
		</class>
</hibernate-mapping>