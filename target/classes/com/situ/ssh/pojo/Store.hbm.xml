<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.situ.ssh.pojo" auto-import="true">
		<class name="Store" table="store">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="name" column="name" type="java.lang.String"></property>
			<property name="location" column="location" type="java.lang.String"></property>
			<property name="amount" column="amount" type="java.lang.String"></property>
			<property name="person" column="person" type="java.lang.String"></property>
		</class>
</hibernate-mapping>