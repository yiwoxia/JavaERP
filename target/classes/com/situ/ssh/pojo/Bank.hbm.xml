<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.situ.ssh.pojo" auto-import="true">
		<class name="Bank" table="bank">
			<id name="id" column="id">
				<generator class="native"></generator>
			</id>
			<property name="payer" column="payer" type="java.lang.String"></property>
			<property name="person" column="person" type="java.lang.String"></property>
			<property name="account" column="account" type="java.lang.String"></property>
			<property name="accountName" column="accountName" type="java.lang.String"></property>
			<property name="money" column="money" type="java.lang.Double"></property>
			<property name="remark" column="remark" type="java.lang.String"></property>
			<property name="time" column="time" type="java.util.Date"></property>
		</class>
</hibernate-mapping>